{
    "name": "eco-service",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "tsx scripts/help.ts tip && next dev --turbopack",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "type": "tsc --noEmit",
        "format": "pnpm exec prettier . --write",
        "db:setup": "tsx scripts/db.ts setup.sql",
        "db:reset": "tsx scripts/db.ts reset.sql",
        "prisma:generate": "pnpm dlx prisma generate",
        "prisma:migrate": "pnpm dlx prisma migrate dev",
        "prisma:deploy": "pnpm dlx prisma migrate deploy",
        "fixtures:setup": "tsx scripts/fixtures.ts setup",
        "fixtures:reset": "tsx scripts/fixtures.ts reset",
        "fixtures:reload": "tsx scripts/fixtures.ts reload",
        "help": "tsx scripts/help.ts help"
    },
    "dependencies": {
        "@plunk/node": "^3.0.3",
        "@prisma/client": "^6.4.1",
        "@react-email/components": "0.0.28",
        "@react-email/render": "^1.0.5",
        "@stripe/stripe-js": "^5.7.0",
        "better-auth": "1.0.7",
        "clsx": "^2.1.1",
        "framer-motion": "^12.4.7",
        "lucide-react": "^0.462.0",
        "next": "^15.2.0",
        "next-zustand": "^1.1.0",
        "nuqs": "^2.4.0",
        "oslo": "^1.2.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-easy-crop": "^5.4.1",
        "stripe": "^17.7.0",
        "swiper": "^11.2.4",
        "tailwind-merge": "^2.6.0",
        "tsx": "^4.19.3",
        "zod": "^3.24.2",
        "zustand": "^5.0.3"
    },
    "devDependencies": {
        "@types/node": "^22.13.5",
        "@types/react": "^18.3.18",
        "@types/react-dom": "^18.3.5",
        "@typescript-eslint/parser": "^8.25.0",
        "eslint": "^8.57.1",
        "eslint-config-next": "15.0.3",
        "eslint-plugin-tailwindcss": "^3.18.0",
        "postcss": "^8.5.3",
        "prettier": "3.5.0",
        "prisma": "^6.4.1",
        "tailwindcss": "^3.4.17",
        "typescript": "^5.7.3"
    },
    "resolutions": {
        "prettier": "3.5.0"
    }
}
